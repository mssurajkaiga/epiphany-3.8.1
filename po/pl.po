msgid "Baltic (_ISO-8859-13)"
msgstr "Bałtyckie (I_SO-8859-13)"

#: ../embed/ephy-encodings.c:59
msgid "Baltic (I_SO-8859-4)"
msgstr "Bałtyckie (I_SO-8859-4)"

